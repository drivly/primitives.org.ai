{
  "name": "ai-observability",
  "version": "0.1.0",
  "description": "Payload CMS instance for observability of AI operations with multiple database support",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'"
  },
  "dependencies": {
    "next": "^14.0.0",
    "payload": "^2.0.0",
    "@payloadcms/db-mongodb": "^3.0.0",
    "@payloadcms/db-postgres": "^3.0.0", 
    "@payloadcms/db-sqlite": "^3.0.0",
    "@payloadcms/richtext-slate": "^1.0.0",
    "vitest": "^1.1.0"
  },
  "peerDependencies": {
    "next": "^14.0.0",
    "payload": "^2.0.0",
    "react": "^18.0.0"
  },
  "devDependencies": {
    "prettier": "^3.0.0",
    "tsup": "^8.4.0",
    "typescript": "^5.0.0",
    "@types/node": "^20.17.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/drivly/primitives.org.ai.git"
  },
  "homepage": "https://primitives.org.ai",
  "bugs": {
    "url": "https://github.com/drivly/primitives.org.ai/issues"
  },
  "keywords": [
    "payload",
    "cms",
    "database",
    "observability",
    "ai"
  ],
  "license": "MIT"
}
